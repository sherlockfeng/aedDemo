.section{
  height: 30px;
  width: 100%;
}
.section input{
  width:90%;
  margin:5px auto;
  border:1px solid #c3c3c3;
  height:30px;
  border-radius: 3px;
  padding: 0 5px;
}
.map_container{
  position: absolute;
  top: 0px;
  bottom: 60px;
  left: 0;
  right: 0;
}
.map_container_img{
  bottom: 115px;
}
.map{
  width: 100%;
  height: 100%;
}
.map_text{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 60px;
  background: #fff;
  padding: 0 15px;
}

.map_text_img{
  bottom: 55px;
}

text{
  margin: 5px 0;
  display: block;
  font-size:12px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.h1{
  margin: 10px 0 5px;
  font-size:15px;
}

.map-1{
  margin-top: 5px;
  width: 100rpx;
  height: 100rpx;
  /* border-radius: 120rpx; */
  background-color: #4D8AD7;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color:#ffffff;
  font-size: 34rpx;
  position: absolute;
  top:0rpx;
  right: 40rpx;
}

.map-1 image{
  width: 60rpx;
  height: 80rpx;
}

.imgList{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 50px;
  background: #fff;
  width:375px
}

.imgList-li {
  display: inline-block;
  margin-right: 10px;
  margin-left: 10px;
}

.img {
  width:45px;
  height: 45px;
  margin-top: 3px;
}

.line {
  background-color: #ddd;
  height: 5px;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 50px;
}

.detail_button{
  margin-top: 35px;
  width: 100rpx;
  height: 40rpx;
  border-radius: 10rpx;
  background-color: #4D8AD7;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color:#ffffff;
  font-size: 24rpx;
  position: absolute;
  top:0rpx;
  right: 60rpx;
}
.detail_button2{
  margin-top: 5px;
  width: 100rpx;
  height: 40rpx;
  border-radius: 10rpx;
  background-color: #4D8AD7;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color:#ffffff;
  font-size: 24rpx;
  position: absolute;
  top:0 rpx;
  right: 60rpx;
}